Proceso cobroestacionamiento
	//variables
	Definir numhr, cobro Como Real;
	//Entrada
	Escribir "el estacionamiento tiene valor de ingreso de 500 $ y valor por hora 600 $";
	Escribir "ingrese cantidad de horas";
	leer numhr;
	//Proceso 
	cobro <- (600*numhr)+500;
	//Salida
	Escribir "precio= ", cobro, "$";
	
FinProceso
